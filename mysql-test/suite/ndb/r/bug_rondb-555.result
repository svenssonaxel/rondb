Create table t1 with a unique index, and table t2 without.
use test;
create table t1 (
id int primary key,
val1 int unique) engine=ndb;
create table t2 (
id int primary key,
val1 int) engine=ndb;
insert into t1 values
(1, 6),
(3, 4),
(2, 5);
insert into t2 values
(8, 11),
(7, 12),
(9, 10);
